{
  "name": "@larastack/frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "pnpm run gen:gql && pnpm run gen:panda",
    "dev": "vite dev",
    "prebuild": "pnpm run gen:gql && pnpm run gen:panda",
    "build": "vite build",
    "start": "vite preview",
    "preview": "vite preview",
    "lint": "oxlint --report-unused-disable-directives-severity error",
    "lint:fix": "oxlint --fix --report-unused-disable-directives-severity error",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "gen:panda": "panda codegen",
    "gen:panda:watch": "panda codegen --watch",
    "gen:gql": "graphql-codegen --config codegen.ts",
    "gen:gql:watch": "graphql-codegen --config codegen.ts --watch",
    "prets:check": "pnpm run gen:gql && pnpm run gen:panda",
    "ts:check": "tsc --pretty --skipLibCheck --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@ark-ui/react": "^5.30.0",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-router": "^1.157.18",
    "@tanstack/react-router-devtools": "^1.157.18",
    "@tanstack/react-start": "^1.157.18",
    "@zag-js/utils": "^1.32.0",
    "apollo-upload-client": "^19.0.0",
    "axios": "^1.13.2",
    "deepmerge": "^4.3.1",
    "graphql": "^16.12.0",
    "js-cookie": "^3.0.5",
    "motion": "^12.24.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.70.0",
    "rxjs": "^7.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/client-preset": "^5.2.2",
    "@graphql-codegen/fragment-matcher": "^6.0.0",
    "@graphql-codegen/introspection": "5.0.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@pandacss/dev": "^1.8.0",
    "@pandacss/shared": "^1.8.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
