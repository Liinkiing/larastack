{
  "name": "@larastack/frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "predev": "graphql-codegen --config codegen.ts && pnpm run gen:panda",
    "dev": "next dev --turbopack",
    "prebuild": "panda codegen && pnpm run gen:gql",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint --no-error-on-unmatched-pattern --cache --fix --report-unused-disable-directives \"src/**/*.{js,ts,jsx,tsx}\" \"app/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "panda codegen",
    "gen:panda:watch": "panda codegen --watch",
    "gen:panda": "panda codegen",
    "gen:gql": "graphql-codegen --config codegen.ts",
    "gen:gql:watch": "graphql-codegen --config codegen.ts --watch",
    "prets:check": "panda codegen && pnpm run gen:gql",
    "ts:check": "tsc --pretty --skipLibCheck --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@ark-ui/react": "^5.30.0",
    "@hookform/resolvers": "^5.2.2",
    "@zag-js/utils": "^1.32.0",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http-common": "^0.2.16",
    "apollo-upload-client": "^17.0.0",
    "axios": "^1.13.2",
    "deepmerge": "^4.3.1",
    "graphql": "^16.12.0",
    "js-cookie": "^3.0.5",
    "motion": "^12.24.7",
    "next": "16.1.1",
    "next-transition-router": "^0.2.11",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.70.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/client-preset": "^5.2.2",
    "@graphql-codegen/fragment-matcher": "^6.0.0",
    "@graphql-codegen/introspection": "5.0.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.1",
    "@graphql-eslint/eslint-plugin": "^4.4.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@pandacss/dev": "^1.8.0",
    "@pandacss/shared": "^1.8.0",
    "@pandacss/studio": "^1.8.0",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/blocks": "^8.6.15",
    "@storybook/nextjs": "^8.6.15",
    "@storybook/nextjs-vite": "^9.1.17",
    "@storybook/react": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@types/apollo-upload-client": "^17.0.5",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "storybook": "^8.6.15",
    "typescript": "^5.9.3"
  }
}
