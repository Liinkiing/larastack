{
  "name": "@larastack/frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "predev": "graphql-codegen --config codegen.ts && pnpm run gen:panda",
    "dev": "next dev --turbopack",
    "prebuild": "panda codegen && pnpm run gen:gql",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "oxlint --report-unused-disable-directives-severity error",
    "lint:fix": "oxlint --fix --report-unused-disable-directives-severity error",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "prepare": "panda codegen",
    "gen:panda:watch": "panda codegen --watch",
    "gen:panda": "panda codegen",
    "gen:gql": "graphql-codegen --config codegen.ts",
    "gen:gql:watch": "graphql-codegen --config codegen.ts --watch",
    "prets:check": "panda codegen && pnpm run gen:gql",
    "ts:check": "tsc --pretty --skipLibCheck --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "^4.1.4",
    "@ark-ui/react": "^5.31.0",
    "@hookform/resolvers": "^5.2.2",
    "@zag-js/utils": "^1.33.1",
    "apollo-upload-client": "^19.0.0",
    "axios": "^1.13.5",
    "deepmerge": "^4.3.1",
    "graphql": "^16.12.0",
    "js-cookie": "^3.0.5",
    "motion": "^12.34.1",
    "next": "16.1.6",
    "next-transition-router": "^0.2.11",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "rxjs": "^7.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/client-preset": "^5.2.2",
    "@graphql-codegen/fragment-matcher": "^6.0.0",
    "@graphql-codegen/introspection": "5.0.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.1",
    "@graphql-eslint/eslint-plugin": "^4.4.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@pandacss/dev": "^1.8.2",
    "@pandacss/shared": "^1.8.2",
    "@pandacss/studio": "^1.8.2",
    "@storybook/addon-docs": "^10.2.9",
    "@storybook/nextjs-vite": "10.2.9",
    "@storybook/react": "^10.2.9",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint-plugin-storybook": "10.2.9",
    "storybook": "^10.2.9",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
