{
  "name": "@larastack/frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "predev": "graphql-codegen --config codegen.ts && pnpm run gen:panda",
    "dev": "next dev",
    "prebuild": "panda codegen && pnpm run gen:gql",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --no-error-on-unmatched-pattern --cache --fix --report-unused-disable-directives \"src/**/*.{js,ts,jsx,tsx}\" \"app/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "panda codegen",
    "gen:panda:watch": "panda codegen --watch",
    "gen:panda": "panda codegen",
    "gen:gql": "graphql-codegen --config codegen.ts",
    "gen:gql:watch": "graphql-codegen --config codegen.ts --watch",
    "prets:check": "panda codegen && pnpm run gen:gql",
    "ts:check": "tsc --pretty --skipLibCheck --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "^3.12.8",
    "@ark-ui/react": "^4.9.1",
    "@hookform/resolvers": "^3.10.0",
    "@zag-js/utils": "^0.82.1",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http-common": "^0.2.16",
    "apollo-upload-client": "^17.0.0",
    "axios": "^1.7.4",
    "deepmerge": "^4.3.1",
    "graphql": "^16.10.0",
    "js-cookie": "^3.0.5",
    "motion": "^12.0.6",
    "next": "15.1.6",
    "next-transition-router": "^0.2.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/client-preset": "^4.6.0",
    "@graphql-codegen/fragment-matcher": "^5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.0",
    "@graphql-eslint/eslint-plugin": "^4.3.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@pandacss/dev": "^0.51.1",
    "@pandacss/shared": "^0.51.1",
    "@pandacss/studio": "^0.51.1",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/addon-interactions": "^8.5.2",
    "@storybook/blocks": "^8.5.2",
    "@storybook/experimental-nextjs-vite": "^8.5.2",
    "@storybook/nextjs": "^8.5.2",
    "@storybook/react": "^8.5.2",
    "@storybook/test": "^8.5.2",
    "@types/apollo-upload-client": "^17.0.5",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.12.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "storybook": "^8.5.2",
    "typescript": "^5"
  }
}
