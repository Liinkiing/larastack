{
  "mcpServers": {
    "laravel-boost": {
      "command": "docker",
      "args": ["exec", "-i", "larastack-app-1", "php", "/var/www/html/artisan", "boost:mcp"]
    },
    "ark-ui": {
      "command": "npx",
      "args": ["-y", "@ark-ui/mcp"]
    },
    "panda": {
      "command": "pnpm",
      "args": ["--filter", "frontend", "exec", "panda", "mcp"]
    }
  }
}
